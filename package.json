    "@tiptap/starter-kit": "^3.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.3.1"
  }
