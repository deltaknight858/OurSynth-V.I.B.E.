{
  "name": "oursynth-eco",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "components/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "format": "prettier --write .",
    "clean": "rimraf dist node_modules",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.8",
    "rimraf": "^5.0.5"
  }
}
