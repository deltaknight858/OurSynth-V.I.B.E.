{
  "name": "oursynth",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm --workspace assist run dev",
    "dev:noteflow": "npm --workspace noteflow run dev",
    "dev:studio": "npm --workspace studio run dev",
    "build": "npm run build:packages && npm --workspace assist run build",
    "build:studio": "npm run build:packages && npm --workspace studio run build",
    "start": "npm --workspace assist run start",
    "start:studio": "npm --workspace studio run start",
    "lint": "next lint",
    "test": "jest --watchAll=false",
    "test:ci": "jest --runInBand",
    "build:core": "npm --workspace @oursynth/core run build",
    "build:agent-runtime": "npm --workspace @oursynth/agent-runtime run build",
    "build:packages": "npm run build:core && npm run build:agent-runtime && npm run build:oai && npm run build:mesh-node && npm run build:capsule && npm run build:wizard-capsule && npm run labs:tokens",
    "build:apps": "npm --workspaces run build",
    "build:oai": "npm --workspace @oursynth/oai run build || echo skipped",
    "build:mesh-node": "npm --workspace @oursynth/mesh-node run build || echo skipped",
    "build:capsule": "npm --workspace @oursynth/capsule run build || echo skipped",
    "build:wizard-capsule": "npm --workspace @oursynth/wizard-capsule run build || echo skipped",
    "labs:tokens": "node scripts/build_tokens.mjs",
    "deploy:capsule": "node scripts/deploy_capsule.mjs",
    "labs:svg": "node scripts/generate_svg_registry.mjs",
    "labs:promote": "node scripts/promote_component.mjs",
    "svg:inventory": "node scripts/svg-inventory.mjs",
    "svg:normalize": "node scripts/svg-normalize.mjs",
    "svg:generate": "node scripts/generate-icon-components.mjs",
    "svg:all": "npm run svg:inventory && npm run svg:normalize && npm run svg:generate",
    "report:svg": "node scripts/svg-inventory.mjs && node scripts/svg-normalize.mjs && node scripts/generate-icon-components.mjs",
    "staging:inventory": "node scripts/staging-inventory.mjs",
    "svg:checklist": "node scripts/generate-icon-checklist.mjs",
    "promote:plan": "node scripts/promote.mjs --plan",
    "promote:phase1": "node scripts/promote.mjs --execute phase=1",
    "lint:colors": "node scripts/color-lint.mjs",
    "refactor:marketing": "node scripts/marketing-extract.mjs",
    "lint:icons": "node scripts/icon-gradient-lint.mjs",
    "lint:tokens": "node scripts/tokens-diff.mjs",
    "ci:design": "npm run lint:colors && npm run lint:icons && npm run lint:tokens && npm run labs:tokens"
  },
  "dependencies": {
    "@oursynth/core": "file:./packages/halo-ui",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.16.10",
    "@mui/lab": "^5.0.0-alpha.177",
    "@mui/material": "^5.16.10",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.57.2",
    "@types/node": "^24.3.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "14.2.3",
    "openai": "^4.104.0",
    "oursynth": "file:",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "18.2.52",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "8.56.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "svgo": "^3.3.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/next": "^8.0.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "typescript": "^5.9.2"
  }
}